{
  "name": "Oikotie",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Misc enhancements for Oikotie.fi in Chrome",
  "homepage_url": "http://example.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://asunnot.oikotie.fi/myytavat-asunnot/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/styles.css"
      ]
    }
  ]
}
